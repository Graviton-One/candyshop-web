.container {
  @media (min-width: theme('screens.xl')) {
    max-width: 1400px;
  }
}
.rainbow {
  position: relative;
  background-image: linear-gradient(269.45deg, #00ffa3 10.33%, #dc1fff 97.14%);
}
.rainbow > * {
  z-index: 2;
}
.rainbow::before {
  content: '';
  box-shadow: inset 0 0 0 150rem #F7F8FA;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 1;
  border-width: 1.5px;
  border-radius: 23px;
  border-color: transparent;
  background-color: transparent;
}
.arrow::after {
  content: '';
  background-image: url("data:image/svg+xml,%3Csvg width='11' height='7' viewBox='0 0 11 7' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath d='M10.0645 1L5.57066 6.26316L1.00013 0.999999' stroke='%2312161D' stroke-linecap='round' stroke-linejoin='round'/%3E%3C/svg%3E%0A");  padding: 0 8px;
  background-repeat: no-repeat;
  position: absolute;
  right: 8px;
  top: 40%;
  z-index: 1;
  text-align: center;
  width: 10%;
  height: 100%;

}
.underline-thickness-0.5 {
